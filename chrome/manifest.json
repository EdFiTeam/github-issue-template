{
  "name": "Github Issue Template",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Backbone for structuring Github Issues",
  "icons": {
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*/issues/new"],
      "js": [
        "script/lib/jquery.min.js",
        "script/new-issue-page.js"
      ]
    }
  ]
}
